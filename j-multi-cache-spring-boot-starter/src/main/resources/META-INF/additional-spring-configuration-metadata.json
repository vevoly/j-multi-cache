{
  "groups": [
    {
      "name": "j-multi-cache",
      "type": "io.github.vevoly.jmulticache.starter.auto_configure.JMultiCacheProperties",
      "sourceType": "io.github.vevoly.jmulticache.starter.auto_configure.JMultiCacheProperties"
    }
  ],
  "properties": [
    {
      "name": "j-multi-cache.enabled",
      "type": "java.lang.Boolean",
